{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "dof-dss/nicsdru_nidirect_theme": "^0.11.1",
        "dof-dss/nicsdru_origins_modules": "^0.8.0",
        "dof-dss/nicsdru_origins_theme": "^0.3.1",
        "dof-dss/nidirect-d8-test-install-profile": "^0.2.2",
        "dof-dss/nidirect-migrations": "^0.10.0",
        "dof-dss/nidirect-site-modules": "^0.11.1",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^2.0",
        "drupal/adminimal_theme": "^1.5",
        "drupal/better_field_descriptions": "^1.5",
        "drupal/block_blacklist": "^1.0",
        "drupal/block_content_permissions": "^1.10",
        "drupal/chosen": "^2.9",
        "drupal/clientside_validation": "^1.2",
        "drupal/config_filter": "^1.4",
        "drupal/config_ignore": "^2.1",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/config_split": "^1.4",
        "drupal/config_update": "^1.6",
        "drupal/console": "^1.9",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/csv_serialization": "^1.4",
        "drupal/diff": "^1.0",
        "drupal/entity_browser": "^2.5",
        "drupal/entity_embed": "^1.0",
        "drupal/entityqueue": "^1.0",
        "drupal/eu_cookie_compliance": "^1.8",
        "drupal/facets": "^1.4",
        "drupal/facets_pretty_paths": "^1.0@beta",
        "drupal/fastly": "^3.9",
        "drupal/field_group": "^3.0",
        "drupal/flag": "4.x-dev",
        "drupal/geocoder": "^2.5",
        "drupal/geolocation": "3.x",
        "drupal/google_analytics_counter": "3.x-dev",
        "drupal/handy_cache_tags": "^1.0",
        "drupal/hms_field": "^1.0@beta",
        "drupal/http_cache_control": "^1.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/layout_builder_modal": "^1.0@alpha",
        "drupal/layout_builder_restrictions": "^2.2",
        "drupal/linkit": "^6.0",
        "drupal/metatag": "^1.8",
        "drupal/migrate_upgrade": "^3.2",
        "drupal/moderation_sidebar": "^1.3",
        "drupal/noreferrer": "^1.7",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.4",
        "drupal/purge": "^3.0",
        "drupal/redirect": "^1.5",
        "drupal/redis": "^1.4",
        "drupal/scheduler": "^1.1",
        "drupal/scheduler_content_moderation_integration": "^1.1",
        "drupal/schema_metatag": "^1.4",
        "drupal/search_api": "^1.14",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/search_api_location": "^1.0@alpha",
        "drupal/search_api_solr": "^3.2",
        "drupal/search_api_spellcheck": "^3.0@alpha",
        "drupal/shs": "^1.0@alpha",
        "drupal/simplei": "^1.1",
        "drupal/smart_trim": "^1.2",
        "drupal/structure_sync": "^1.16",
        "drupal/taxonomy_access_fix": "^2.6",
        "drupal/telephone_plus": "1.x-dev",
        "drupal/token": "^1.5",
        "drupal/token_filter": "^1.1",
        "drupal/twig_extensions": "^2.0",
        "drupal/twig_field_value": "^1.2",
        "drupal/twig_tweak": "^2.4",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/views_contextual_filters_or": "^1.1",
        "drupal/views_custom_cache_tag": "^1.1",
        "drupal/views_data_export": "^1.0",
        "drupal/webform": "^5.4",
        "drupal/whatlinkshere": "^2.0.0",
        "drupal/xls_serialization": "^1.2",
        "drush/drush": "^10.1",
        "harvesthq/chosen": "^1.8",
        "oomphinc/composer-installers-extender": "^1.1",
        "platformsh/config-reader": "^2.3",
        "symfony/event-dispatcher": "4.3.11 as 3.4.35",
        "vlucas/phpdotenv": "^2.4",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8",
        "drupal/devel": "^2.1",
        "drupal/easy_install": "^10.1",
        "drupal/masquerade": "^2.0@beta",
        "drupal/migrate_plus": "^4.2",
        "drupal/migrate_tools": "4.x-dev",
        "drupal/multiline_config": "^1.0",
        "drupal/permissions_filter": "^1.1",
        "drupal/restui": "^1.17",
        "drupal/stage_file_proxy": "^1.0@beta",
        "mglaman/drupal-check": "^1.0",
        "phpstan/phpstan": "0.11.16",
        "previousnext/phpunit-finder": "^1.0@alpha"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "patches": {
            "drupal/core": {
                "Prevent leftover fields from causing errors like 'Call to a member function getLabel() after enabling layout_builder'": "https://www.drupal.org/files/issues/2020-01-20/2985882-53.patch",
                "When embedding media, don't let authors choose view modes that are not enabled for that media type": "https://www.drupal.org/files/issues/2020-01-31/3097416-ckeditor-view-modes-8.9-combined.patch",
                "Prevent error when saving unpublished content for users without manage book privileges": "https://www.drupal.org/files/issues/2918537-36.patch",
                "[PP-1] Provide options to sanitize filenames (transliterate, lowercase, replace whitespace, etc)": "https://www.drupal.org/files/issues/2019-12-06/2492171-294.patch"
            },
            "drupal/config_split": {
                "General typo issues": "https://www.drupal.org/files/issues/2018-10-02/general_typo_issues-3003865-4.patch",
                "Capitalize the name of the module in .info.yml file": "https://www.drupal.org/files/issues/2018-10-05/config_split-info-3004751-2.patch",
                "Config export fails on Drush 9 with config_translation": "https://www.drupal.org/files/issues/2018-11-05/config_export_fails_on-3011363-2.patch"
            },
            "drupal/easy_install": {
                "Handle missing core version": "https://www.drupal.org/files/issues/2019-12-19/easy_install_core_version-3101883-0.patch"
            },
            "drupal/google_analytics_counter": {
                "Add API query tweak options": "https://gist.githubusercontent.com/omahm/02ac3b7138c4d0518bd954721b74b763/raw/8d7ffa08b21359c7bf11165cdef1fb7e98483774/gac_query_options.patch"
            },
            "drupal/redirect": {
                "URL redirects listing only displays operations": "https://www.drupal.org/files/issues/2019-04-05/redirects_list_builder-3031125-9-d8.patch"
            },
            "drupal/search_api_spellcheck": {
                "Suggestions Spellcheck views area plugin always hidden": "https://www.drupal.org/files/issues/2020-01-29/suggestions-spellcheck-always-hidden-1.patch"
            },
            "drupal/clientside_validation": {
                "Clientside validation errors revert to being output as a label in webform wizard pages with ajax enabled": "https://www.drupal.org/files/issues/2020-02-03/cvjquery-ajax-webform-wizard.patch",
                "Fix LinkIt module errors": "https://gist.githubusercontent.com/omahm/5a162e438abb89403dac459e7f2d7597/raw/78dac8ddc3f4e1c32289fc23293f91faaf329548/3124809.patch"
            },
            "drupal/password_policy": {
                "field_last_password_reset is unknown while importing configuration on site without password_policy activated": "https://www.drupal.org/files/issues/2020-01-17/password_policy-n2771129-109.patch"
            },
            "drupal/paragraphs": {
                "Integrity constraint violation: 1048 Column 'langcode' cannot be null": "https://www.drupal.org/files/issues/2020-06-25/paragraphs-2901390-51.patch"
            },
            "drupal/shs": {
                "SHS select is empty on views": "https://www.drupal.org/files/issues/2018-12-10/shs-blank-select-fix-3019378-3.patch"
            }
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/README.txt": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/example.gitignore": false
            }
        },
        "installer-types": [
            "library"
        ],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "harvesthq/chosen"
            ],
            "web/modules/custom": ["dof-dss/nidirect-site-modules"],
            "web/modules/origins": ["dof-dss/nicsdru_origins_modules"],
            "web/modules/migrate/{$name}": ["dof-dss/nidirect-migrations"],
            "web/profiles/custom": ["dof-dss/nidirect-d8-test-install-profile"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, you’ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
