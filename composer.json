{
    "name": "nics/nidirect",
    "description": "NI Direct Drupal 8 - Based on drupal-composer/drupal-project",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        },
        {
            "type": "github",
            "url": "git@github.com:dof-dss/nidirect-d8-mig-mods.git",
            "no-api": true
        },
        {
            "type": "github",
            "url": "git@github.com:dof-dss/nidirect-site-modules.git",
            "no-api": true
        },
        {
            "type": "github",
            "url": "git@github.com:dof-dss/nidirect-d8-test-install-profile.git",
            "no-api": true
        },
        {
            "type": "github",
            "url": "git@github.com:dof-dss/nicsdru_origins_theme.git",
            "no-api": true
        },
        {
            "type": "github",
            "url": "git@github.com:dof-dss/nicsdru_nidirect_theme.git",
            "no-api": true
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "dof-dss/nicsdru_nidirect_theme": "^0.2.0",
        "dof-dss/nicsdru_origins_modules": "^0.3",
        "dof-dss/nicsdru_origins_theme": "^0.1.3",
        "dof-dss/nidirect-site-modules": "^0.1.0",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^2.0",
        "drupal/adminimal_theme": "^1.5",
        "drupal/block_blacklist": "^1.0",
        "drupal/chosen": "^2.9",
        "drupal/clientside_validation": "^1.2",
        "drupal/config_filter": "^1.4",
        "drupal/config_ignore": "^2.1",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/config_split": "^1.4",
        "drupal/config_update": "^1.6",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.8",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/diff": "^1.0",
        "drupal/entity_embed": "1.0",
        "drupal/entityqueue": "^1.0@beta",
        "drupal/eu_cookie_compliance": "^1.8",
        "drupal/facets": "^1.4",
        "drupal/facets_pretty_paths": "^1.0@beta",
        "drupal/field_group": "^3.0",
        "drupal/flag": "4.x-dev",
        "drupal/geocoder": "^2.5",
        "drupal/geolocation": "3.x",
        "drupal/google_analytics_counter": "3.x-dev",
        "drupal/handy_cache_tags": "^1.0",
        "drupal/hms_field": "^1.0@beta",
        "drupal/http_cache_control": "^1.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/layout_builder_modal": "^1.0@alpha",
        "drupal/layout_builder_restrictions": "^2.2",
        "drupal/linkit": "^4.3",
        "drupal/media_browser_responsive_images": ">1.x-dev",
        "drupal/media_entity_browser": "^2.0",
        "drupal/metatag": "^1.8",
        "drupal/noreferrer": "^1.7",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.4",
        "drupal/redirect": "^1.3",
        "drupal/redis": "^1.4",
        "drupal/scheduler": "^1.1",
        "drupal/scheduler_content_moderation_integration": "^1.1",
        "drupal/schema_metatag": "^1.4",
        "drupal/search_api": "^1.14",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/search_api_location": "^1.0@alpha",
        "drupal/search_api_solr": "^3.2",
        "drupal/search_api_spellcheck": "^3.0@alpha",
        "drupal/shs": "^1.0@alpha",
        "drupal/simplei": "^1.1",
        "drupal/smart_trim": "^1.2",
        "drupal/structure_sync": "^1.16",
        "drupal/taxonomy_access_fix": "^2.6",
        "drupal/taxonomy_manager": "^1.0@alpha",
        "drupal/telephone_plus": "1.x-dev",
        "drupal/token": "^1.5",
        "drupal/token_filter": "^1.1",
        "drupal/twig_extensions": "^2.0",
        "drupal/twig_field_value": "^1.2",
        "drupal/twig_tweak": "^2.4",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/views_contextual_filters_or": "^1.1",
        "drupal/views_custom_cache_tag": "^1.1",
        "drupal/webform": "^5.4",
        "drupal/whatlinkshere": "^1.0",
        "drush/drush": "^9.0.0",
        "harvesthq/chosen": "^1.8",
        "oomphinc/composer-installers-extender": "^1.1",
        "symfony/event-dispatcher": "4.3.4 as 3.4.35",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "dof-dss/nidirect-d8-test-install-profile": "^0.1.1",
        "dof-dss/nidirect-migrations": "^0.1.1",
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^8.8",
        "drupal/devel": "^2.1",
        "drupal/easy_install": "^10.1",
        "drupal/migrate_plus": "^4.2",
        "drupal/migrate_tools": "4.x-dev",
        "drupal/migrate_upgrade": "^3.0",
        "drupal/permissions_filter": "^1.1",
        "drupal/restui": "^1.17",
        "drupal/stage_file_proxy": "^1.0@beta",
        "mglaman/drupal-check": "^1.0",
        "phpstan/phpstan": "0.11.16",
        "previousnext/phpunit-finder": "^1.0@alpha"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "dof-dss/*": "source",
            "*": "dist"
        },
        "platform": {
            "php": "7.1.19"
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Composer\\": "composer"
        }
    },
    "extra": {
        "patches": {
            "drupal/config_split": {
                "General typo issues": "https://www.drupal.org/files/issues/2018-10-02/general_typo_issues-3003865-4.patch",
                "Capitalize the name of the module in .info.yml file": "https://www.drupal.org/files/issues/2018-10-05/config_split-info-3004751-2.patch",
                "Config export fails on Drush 9 with config_translation": "https://www.drupal.org/files/issues/2018-11-05/config_export_fails_on-3011363-2.patch"
            },
            "drupal/facets": {
                "Hide facets when it is configured to hide the block without losing support for bigpipe and ajax facets": "https://www.drupal.org/files/issues/2019-06-11/facets-empty-block-2984465-31.patch"
            },
            "drupal/taxonomy_manager": {
                "Add search feature to taxonomy admin form": "https://www.drupal.org/files/issues/2019-08-06/add_search_feature-2906768-21.patch",
                "Parameter must be an array": "https://www.drupal.org/files/issues/2019-01-14/taxonomy_manager-dev-parameter-count-3025793-3.patch",
                "Update jquery.fancytree library to version 2.30.1": "https://www.drupal.org/files/issues/2019-01-10/taxonomy_manager-d8-jquery.fancytree_update-3025020-2.patch"
            },
            "drupal/media_browser_responsive_images": {
                "Form alter applies for any media browser, not just image related ones": "https://www.drupal.org/files/issues/2019-12-17/media-browser-hook-checks-for-image-types.patch"
            },
            "drupal/easy_install": {
                "Handle missing core version": "https://www.drupal.org/files/issues/2019-12-19/easy_install_core_version-3101883-0.patch"
            },
            "drupal/google_analytics_counter": {
                "Add API query tweak options": "https://gist.githubusercontent.com/omahm/02ac3b7138c4d0518bd954721b74b763/raw/8d7ffa08b21359c7bf11165cdef1fb7e98483774/gac_query_options.patch",
                "Fix missing node_type error on install": "https://www.drupal.org/files/issues/2019-11-12/google_analytics_counter-fix_missing_dependency-3093910-3.patch"
            },
            "drupal/redirect": {
                "URL redirects listing only displays operations": "https://www.drupal.org/files/issues/2019-04-05/redirects_list_builder-3031125-9-d8.patch"
            },
            "drupal/search_api_spellcheck": {
                "Suggestions Spellcheck views area plugin always hidden": "https://www.drupal.org/files/issues/2020-01-29/suggestions-spellcheck-always-hidden-1.patch"
            },
            "drupal/webform": {
                "Required radios data-msg-required does not match data-webform-required-error when radios are conditionally visible": "https://www.drupal.org/files/issues/2020-01-27/webform.element.radios.data-msg-required.patch",
                "Provide support for complete list of autocomplete attribute values defined in HTML 5.2 spec": "https://www.drupal.org/files/issues/2020-02-13/3113160-4.patch"
            },
            "drupal/clientside_validation": {
                "Cientside validation errors revert to being output as a label in webform wizard pages with ajax enabled": "https://www.drupal.org/files/issues/2020-02-03/cvjquery-ajax-webform-wizard.patch"
            }
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": ["library"],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "harvesthq/chosen"
            ],
            "web/modules/custom": [
                "dof-dss/nidirect-site-modules"
            ],
            "web/modules/origins": [
                "dof-dss/nicsdru_origins_modules"
            ],
            "web/modules/migrate/{$name}": [
                "dof-dss/nidirect-migrations"
            ],
            "web/profiles/custom": [
                "dof-dss/nidirect-d8-test-install-profile"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        }
    }
}
