{
    "name": "nics/nidirect",
    "description": "NI Direct Drupal 8 - Based on drupal-composer/drupal-project",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dof-dss/nidirect-d8-mig-mods"
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        },
        {
            "type": "vcs",
            "url": "git@github.com:dof-dss/nidirect-site-modules.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dof-dss/nidirect-d8-test-install-profile.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dof-dss/nicsdru_origins_theme.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dof-dss/nicsdru_nidirect_theme.git"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^1.26",
        "drupal/adminimal_theme": "^1.5",
        "drupal/chosen": "^2.7",
        "drupal/config_filter": "^1.4",
        "drupal/config_ignore": "^2.1",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/config_split": "^1.4",
        "drupal/config_update": "^1.6",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drupal/entity_embed": "1.0",
        "drupal/field_group": "^1.0",
        "drupal/flag": "4.x-dev",
        "drupal/geolocation": "^1.11",
        "drupal/hms_field": "^1.0@beta",
        "drupal/http_cache_control": "^1.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/linkit": "^4.3",
        "drupal/media_entity_browser": "^2.0",
        "drupal/memcache": "^2.0",
        "drupal/metatag": "^1.8",
        "drupal/paragraphs": "^1.8",
        "drupal/paragraphs_inline_entity_form": "^1.0@beta",
        "drupal/pathauto": "^1.4",
        "drupal/redirect": "^1.3",
        "drupal/search_api": "^1.14",
        "drupal/search_api_solr": "^3.2",
        "drupal/shs": "^1.0@alpha",
        "drupal/simplei": "^1.1",
        "drupal/token": "^1.5",
        "drupal/token_filter": "^1.1",
        "drupal/traffic_light_rating": "1.x-dev",
        "drupal/twig_extensions": "^2.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drush/drush": "^9.0.0",
        "harvesthq/chosen": "^1.8",
        "nics/nidirect-site-modules": "dev-development",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0",
        "dof-dss/nicsdru_origins_theme": "dev-master",
        "dof-dss/nicsdru_nidirect_theme": "dev-master"
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "drupal/devel": "^2.1",
        "drupal/easy_install": "^10.1",
        "drupal/migrate_plus": "^4.2",
        "drupal/migrate_tools": "^4.1",
        "drupal/migrate_upgrade": "^3.0",
        "drupal/restui": "^1.17",
        "drupal/stage_file_proxy": "^1.0@beta",
        "nics/nidirect-migrations": "dev-development",
        "nics/nidirect-d8-test-install-profile": "dev-development",
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "platform": {
            "php": "7.1.19"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/custom": [
                "nics/nidirect-site-modules"
            ],
            "web/modules/migrate/{$name}": [
                "nics/nidirect-migrations"
            ],
            "web/profiles/custom": [
                "nics/nidirect-d8-test-install-profile"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
