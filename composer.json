{
    "name": "nics/nidirect",
    "description": "NI Direct Drupal 8 - Based on drupal-composer/drupal-project",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "git@svegit01.thestables.net:dss/nidirect-d8-mig-mods"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^1.26",
        "drupal/chosen": "^2.7",
        "drupal/config_filter": "^1.4",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/config_split": "^1.4",
        "drupal/config_update": "^1.6",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/geolocation": "^1.11",
        "drupal/google_map_field": "^1.5",
        "drupal/hms_field": "^1.0@beta",
        "drupal/http_cache_control": "^1.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/media_entity_browser": "^2.0",
        "drupal/memcache": "^2.0",
        "drupal/metatag": "^1.8",
        "drupal/pathauto": "^1.4",
        "drupal/redirect": "^1.3",
        "drupal/shs": "^1.0@alpha",
        "drupal/simplei": "^1.1",
        "drupal/token": "^1.5",
        "drupal/traffic_light_rating": "1.x-dev",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "drupal/devel": "^2.1",
        "drupal/easy_install": "^10.1",
        "drupal/migrate_plus": "^4.2",
        "drupal/migrate_tools": "^4.1",
        "drupal/migrate_upgrade": "^3.0",
        "drupal/stage_file_proxy": "^1.0@beta",
        "nics/nidirect-migrations": "dev-development",
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/migrate/{$name}": [
                "nics/nidirect-migrations"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
