    "require-dev": {
        "drupal/devel": "^5.0",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/easy_install": "^10.5",
        "drupal/fpa": "^3.0",
        "drupal/masquerade": "^2.0@beta",
        "drupal/migrate_tools": "^5.0",
